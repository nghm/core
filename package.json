{
  "name": "nghm",
  "version": "$VERSION",
  "repository": {
    "url": "https://github.com/nghm/platform.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:all": "npm run build:core && npm run build:forms && npm run build:material-fields && npm run build:router && npm run build:material-devpage",
    "build:core": "ng build core --prod",
    "build:forms": "ng build forms --prod",
    "build:material-devpage": "ng build material-devpage --prod",
    "build:material-fields": "ng build material-fields --prod",
    "build:router": "ng build router --prod",
    "test:core": "ng test core --browsers=ChromeHeadless --progress=false",
    "test:forms": "ng test forms --browsers=ChromeHeadless --progress=false",
    "test:material-devpage": "ng test material-devpage --browsers=ChromeHeadless --progress=false",
    "test:material-fields": "ng test material-fields --browsers=ChromeHeadless --progress=false",
    "test:router": "ng test router --browsers=ChromeHeadless --progress=false",
    "test:nghm": "ng test nghm --browsers=ChromeHeadless --progress=false",
    "lint:core": "ng lint core",
    "lint:forms": "ng lint forms",
    "lint:material-devpage": "ng lint material-devpage",
    "lint:material-fields": "ng lint material-fields",
    "lint:router": "ng lint router",
    "lint:nghm": "ng lint nghm",
    "echo:version": "echo $VERSION",
    "compodoc:core": "./node_modules/.bin/compodoc -p projects/core/tsconfig.lib.json -d docs/v$VERSION/core --hideGenerator --disableSourceCode --disableGraph --disablePrivate --disableProtected --disableInternal --theme=vagrant",
    "compodoc:forms": "./node_modules/.bin/compodoc -p projects/forms/tsconfig.lib.json -d docs/v$VERSION/forms --hideGenerator --disableSourceCode --disableGraph --disablePrivate --disableProtected --disableInternal --theme=vagrant",
    "compodoc:material-fields": "./node_modules/.bin/compodoc -p projects/material-fields/tsconfig.lib.json -d docs/v$VERSION/material-fields --hideGenerator --disableSourceCode --disableGraph --disablePrivate --disableProtected --disableInternal --theme=vagrant",
    "compodoc:material-devpage": "./node_modules/.bin/compodoc -p projects/material-devpage/tsconfig.lib.json -d docs/v$VERSION/material-devpage --hideGenerator --disableSourceCode --disableGraph --disablePrivate --disableProtected --disableInternal --theme=vagrant",
    "compodoc:router": "./node_modules/.bin/compodoc -p projects/router/tsconfig.lib.json -d docs/v$VERSION/router --hideGenerator --disableSourceCode --disableGraph --disablePrivate --disableProtected --disableInternal --theme=vagrant",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "css-select": "^2.0.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@compodoc/compodoc": "^1.1.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
